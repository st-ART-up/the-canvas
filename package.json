    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "blessed": "^0.1.81",
    "chalk": "^2.4.2",
    "display-image": "^1.1.0",
    "dotenv": "^8.2.0",
    "dotenv-config": "^0.1.1",
    "express": "^4.17.1",
